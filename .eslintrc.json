{ 
  "printWidth": 80,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "nome",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowsParens": "avoid",
  "overrides": [{
    "files": "*.xml"
  }],
  "extends": [
    "next/core-web-vitals",
    "plugin:prettier/recommended"
  ]
}


